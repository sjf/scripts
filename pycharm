#!/bin/bash
#Generated by JetBrains Toolbox 3.3.0.74186 at 2026-02-26T09:01:17

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/sjf/Applications/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${intellij_args[@]}"